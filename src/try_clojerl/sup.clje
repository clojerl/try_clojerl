(ns try-clojerl.sup)

(defn start-link []
  (supervisor/start_link.e #erl[:local :try-clojerl.sup]
                           :try-clojerl.sup
                           #erl()))

(defn init [args]
  (let [sup-flags   #erl{:strategy :one_for_one
                         :intensity 1
                         :period 5}
        child-specs #erl()]
    #erl[:ok #erl[sup-flags child-specs]]))
